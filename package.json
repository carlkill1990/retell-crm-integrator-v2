{
  "name": "retell-sync",
  "version": "1.0.0",
  "description": "Scalable integration platform connecting Retell AI voice agents with popular CRMs",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
